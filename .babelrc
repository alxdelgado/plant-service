{
    "env": {
        "development": {
            "compact": true
        }
    },
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "babel-plugin-styled-components",
        [
            "babel-plugin-dotenv",
            {
                "replacedModuleName": "babel-dotenv"
            }
        ]
    ]
}